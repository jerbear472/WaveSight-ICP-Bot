# ICPScope Environment Configuration

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Proxy Configuration
PROXY_PROVIDER=your_proxy_provider
PROXY_USERNAME=your_username
PROXY_PASSWORD=your_password
PROXY_ENDPOINT=http://proxy.provider.com:8000

# Platform Credentials (Optional - for API access)
INSTAGRAM_SESSION_ID=optional_session_id
TIKTOK_SESSION_ID=optional_session_id

# Bot Configuration
MAX_CONCURRENT_BOTS=5
BOT_TIMEOUT_MS=300000
SCROLL_DELAY_MIN=2000
SCROLL_DELAY_MAX=5000
HUMAN_BEHAVIOR_VARIANCE=0.3

# Analytics Configuration
TREND_UPDATE_INTERVAL_MINUTES=30
DATA_RETENTION_DAYS=90

# Dashboard Configuration
DASHBOARD_PORT=3000
API_PORT=3001
JWT_SECRET=your_jwt_secret_key

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/icpscope.log

# Development
NODE_ENV=development
DEBUG=icpscope:*