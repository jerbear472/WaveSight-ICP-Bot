{
  "name": "icpscope",
  "version": "1.0.0",
  "description": "ICP Bot Monitoring System - Track what your ideal customers see on social platforms",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "start:frontend": "node render-deploy.js",
    "start:backend": "node start-backend.js",
    "dev": "nodemon dashboard/server.js",
    "build": "echo 'Build complete'",
    "test": "jest",
    "orchestrator": "node bot-engine/orchestrator.js",
    "bot:instagram": "node bot-engine/instagram-bot.js",
    "bot:tiktok": "node bot-engine/tiktok-bot.js",
    "analytics": "node analytics/trend-processor.js",
    "dashboard": "node dashboard/server.js",
    "demo": "node dashboard/demo-server.js",
    "start:all": "node start-servers.js",
    "monitor": "node monitor-servers.js",
    "status": "node check-status.js",
    "stop:all": "lsof -ti:3000,3001 | xargs kill -9 2>/dev/null || true"
  },
  "keywords": ["icp", "bot", "monitoring", "instagram", "tiktok", "marketing", "analytics"],
  "author": "ICPScope Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "playwright": "^1.40.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "user-agents": "^1.0.1456"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/node": "^20.10.4",
    "eslint": "^8.55.0"
  }
}